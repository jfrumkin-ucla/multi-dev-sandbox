{% extends "block.html.twig" %}
{#
/**
 * @file
 * Theme override for a Marketing Highlight - Horizontal.
 *
 * Available variables:
 * - Look in the block.html.twig file for available variables.
 */
#}
{% set label = FALSE %}
{% set class_panel = TRUE %}

{% block content %}

    {% include "@siteden-components/card.html.twig" with {
      image: content.field_sf_m_block_primary_img,
      title: content.field_sf_title.0['#context'].value,
      text: content.field_sf_mh_description.0['#context'].value,
      link: {
        title: content.field_sf_mh_call_to_action.0['#context'].value,
        url: content.field_sf_link.0['#url']
      }
    } %}
{% endblock %}
