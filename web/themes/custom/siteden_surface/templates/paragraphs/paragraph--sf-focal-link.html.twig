{% extends "paragraph.html.twig" %}
{#
/**
 * @file
 * Theme override for a Focal link paragraph.
 *
 * Available variables:
 * - Look in the paragraph.html.twig file for available variables.
 */
#}

{% block content %}
  {% include '@siteden-components/focal-link.html.twig' with {
    title: content.field_sf_title.0,
    url: content.field_sf_link.0['#url'],
    use_custom: content.field_sf_focal_custom.0['#markup'],
    image: content.field_sf_image.0,
    icon: content.field_sf_focal_icon.0['#markup'],
  } %}
{% endblock %}
