{% extends "paragraph.html.twig" %}
{#
/**
 * @file
 * Theme override for an Iframe embed paragraph.
 *
 * Available variables:
 * - Look in the paragraph.html.twig file for available variables.
 */
#}

{% block content %}
  {% include '@siteden-components/iframe.html.twig' with {
    url: content.field_sf_link.0['#url'],
    description: content.field_sf_description.0,
    width: content.field_sf_width.0['#markup'],
    height: content.field_sf_height.0['#markup'],
    constrain_width: content.field_sf_constrain_width.0['#markup'],
  } %}
{% endblock %}
