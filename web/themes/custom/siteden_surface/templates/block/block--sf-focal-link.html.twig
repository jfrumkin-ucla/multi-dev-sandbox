{% extends "block.html.twig" %}
{#
/**
 * @file
 * Theme override for a Focal Link block.
 *
 * Available variables:
 * - Look in the block.html.twig file for available variables.
 */
#}

{% block content %}
  {% include '@siteden-components/focal-link.html.twig' with {
    title: content.field_sf_title.0,
    url: content.field_sf_link.0['#url'],
    use_custom: content.field_sf_icon_choice.0['#markup'] ? 0 : 1,
    image: content.field_sf_m_block_primary_img.0,
    icon: content.field_sf_icons.0,
  } %}
{% endblock %}
