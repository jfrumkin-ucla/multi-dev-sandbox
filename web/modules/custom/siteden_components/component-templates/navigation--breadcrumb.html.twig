{#<div class="ucla campus">#}
  <ul class="breadcrumb">
    {% for item in breadcrumb %}
      {% if loop.first %}
      <li class="breadcrumb__item breadcrumb__item--first">
      {% else %}
      <li class="breadcrumb__item">
      {% endif %}
      {% if item.url %}
        <a href="{{ item.url }}">{{ item.text }}</a>
      {% else %}
        {{ item.text }}
      {% endif %}
      </li>
    {% endfor %}
  </ul>
{#</div>#}
